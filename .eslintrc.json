{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
        "document": true,
        "window": true,
        "fetch": true
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "react/forbid-prop-types": 0,
        "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "to" ]
        }],
        "jsx-a11y/label-has-for": 0,
        "no-shadow": 0,
        "react/jsx-filename-extension": 0
    }
  }